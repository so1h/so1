!include "..\..\MKFILE0"

PROGRAMA = ESCR_S

disquete: ..\..\DISQUETE\$(PROGRAMA).bin

..\..\DISQUETE\$(PROGRAMA).bin: $(PROGRAMA).bin
  @if exist $@ del $@
  copy $** $:

$(PROGRAMA).bin: $(PROGRAMA).org
  @echo.
  @copy $(PROGRAMA).org $(PROGRAMA).BIN

clean:
  @if exist $(PROGRAMA).bin erase $(PROGRAMA).bin

build: 
  @rem

show:
  @rem

hide:
  @rem
  
delsrc:
  @rem
  
run: disquete
  cd ..\..\disquete
  so1.exe
  
